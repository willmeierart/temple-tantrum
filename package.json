{
  "name": "agency-zero-starter-v4",
  "version": "1.0.0",
  "description": "using next.js and apollo",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "dev": "node server/index.js",
    "next": "next",
    "build": "next build",
    "start": "NODE_ENV=production node server/index.js",
    "docker:build": "docker build -t <containername> .",
    "docker:clean": "docker rm -f <containername> || true",
    "docker:run": "docker run -p 7878:3000 -d --name <containername> <containername> && npm run docker:logs",
    "docker:stop": "docker stop <containername>",
    "docker:start": "docker start <containername> && npm run docker:logs",
    "docker:logs": "docker logs -f <containername>",
    "deploy": "npm run docker:build && npm run docker:clean && npm run docker:run"
  },
  "author": "Will Meier",
  "license": "ISC",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**"
    ]
  },
  "dependencies": {
    "apollo-client-preset": "^1.0.8",
    "autoprefixer": "^7.2.6",
    "compression": "^1.7.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "graphql": "^0.11.7",
    "graphql-anywhere": "^4.1.8",
    "graphql-tag": "^2.9.1",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-unfetch": "^2.0.0",
    "loaders.css": "^0.1.2",
    "next": "^5.1.0",
    "next-routes": "^1.4.1",
    "node-sass": "^4.8.3",
    "normalize.css": "^7.0.0",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-apollo": "^2.1.3",
    "react-dom": "^16.3.2",
    "react-loaders": "^3.0.1",
    "react-redux": "^5.0.7",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "chai": "^4.1.2",
    "chai-jquery": "^2.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "glob": "^7.1.2",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jquery": "^3.3.1",
    "jsdom": "^11.8.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.4",
    "prop-types": "^15.6.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^6.0.7",
    "standard": "^10.0.3"
  }
}
